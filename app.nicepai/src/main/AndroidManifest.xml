<?xml version="1.0" encoding="utf-8"?>
<manifest package="me.aheadlcx.nicepai"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="me.aheadlcx.plugin.nicepai"/>

    <application
        android:name="me.aheadlcx.app.nicepai.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        xmlns:tools="http://schemas.android.com/tools"
        tools:replace="android:name"
        android:theme="@style/AppTheme">
        <activity android:name="me.aheadlcx.app.nicepai.MainActivity">

        </activity>

        <activity android:name="me.aheadlcx.app.nicepai.ui.cate.GoddessAct"/>
        <activity android:name="me.aheadlcx.app.nicepai.GuideAct">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="me.aheadlcx.app.nicepai.ui.CateDetailAct"
            android:screenOrientation="portrait"
            >
            <!--android:screenOrientation="landscape"-->

        </activity>

        <activity
            android:name="me.aheadlcx.app.nicepai.ui.catelist.CateMainAct"
            android:theme="@style/AppTheme"
            >
            <intent-filter>
                <action android:name="me.aheadlcx.plugin.nicepai"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name="me.aheadlcx.app.nicepai.test.TestDaggerAct">

        </activity>
    </application>

</manifest>
